prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: libxfce4smclient-private
Description: Private Xfce library containing Gnome's Egg sm client until there is an implementation in Gtk+
Requires: gdk-2.0 gtk+-2.0
Version: @PACKAGE_VERSION@
Libs: @LIBX11_LDFLAGS@ -L${libdir} -lxfce4smclient-private-@LIBXFCE4SMCLIENT_PRIVATE_VERSION_API@
Cflags: @LIBX11_CFLAGS@ -I${includedir}/xfce4/libxfce4smclient-private-@LIBXFCE4SMCLIENT_PRIVATE_VERSION_API@
